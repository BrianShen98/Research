import string, sys

def findBad():
    for i in range(469):
        with open("./progRefined/{}.clauses".format(i),"r") as inFile:
            for line in inFile:
                if "sent:" in line and "clause dscr:" not in next(inFile):
                    print("File {}\n{}".format(i,line))


            
                    
                    

